{
  "name": "@type-cacheable/ioredis-adapter",
  "version": "8.0.1",
  "description": "Adapter for using ioredis with type-cacheable",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "node"
    ]
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "node ./dist/index.js",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch --runInBand",
    "build": "npm run clean && tsc",
    "prepare": "npm run-script build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuaslate/type-cacheable.git"
  },
  "keywords": [
    "cache",
    "typescript",
    "ioredis"
  ],
  "author": "Joshua Slate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshuaslate/type-cacheable/issues"
  },
  "homepage": "https://github.com/joshuaslate/type-cacheable#readme",
  "devDependencies": {
    "@types/ioredis": "^4.14.4",
    "@types/jest": "^26.0.0",
    "ioredis": "^4.14.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "ioredis": "^4.14.1"
  },
  "dependencies": {
    "@type-cacheable/core": "^8.0.1",
    "compare-versions": "^3.6.0"
  }
}
